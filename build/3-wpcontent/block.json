{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "iori/monocontent",
	"parent": ["iori/monolayer"],
	"version": "1.0.0",
	"title": "Core Blocks",
	"category": "monoblock",
	"description": "Add core blocks.",
	"supports": {
		"html": false,
		"color": {
			"text": false,
			"background": false
		}
	},
	"attributes": {
		"visibility": {
			"type": "boolean",
			"default": true
		},
		"left": {
			"type": "object",
			"default": {
				"Desktop": { "value": 10, "unit": "px", "use": true },
				"Tablet": { "value": 10, "unit": "px", "use": true },
				"Mobile": { "value": 10, "unit": "px", "use": true },
				"isResponsive": false
			}
		},
		"top": {
			"type": "object",
			"default": {
				"Desktop": { "value": 10, "unit": "px", "use": true },
				"Tablet": { "value": 10, "unit": "px", "use": true },
				"Mobile": { "value": 10, "unit": "px", "use": true },
				"isResponsive": false
			}
		},
		"right": {
			"type": "object",
			"default": {
				"Desktop": { "value": 10, "unit": "px", "use": false },
				"Tablet": { "value": 10, "unit": "px", "use": false },
				"Mobile": { "value": 10, "unit": "px", "use": false },
				"isResponsive": false
			}
		},
		"bottom": {
			"type": "object",
			"default": {
				"Desktop": { "value": 10, "unit": "px", "use": false },
				"Tablet": { "value": 10, "unit": "px", "use": false },
				"Mobile": { "value": 10, "unit": "px", "use": false },
				"isResponsive": false
			}
		},
		"horizontalAlign": {
			"type": "object",
			"default": {
				"Desktop": { "use": false },
				"Tablet": { "use": false },
				"Mobile": { "use": false },
				"isResponsive": false
			}
		},
		"verticalAlign": {
			"type": "object",
			"default": {
				"Desktop": { "use": false },
				"Tablet": { "use": false },
				"Mobile": { "use": false },
				"isResponsive": false
			}
		},
		"width": {
			"type": "object",
			"default": {
				"Desktop": { "value": 275, "unit": "px" },
				"Tablet": { "value": 275, "unit": "px" },
				"Mobile": { "value": 275, "unit": "px" },
				"isResponsive": false
			}
		},
		"height": {
			"type": "object",
			"default": {
				"Desktop": { "value": 0, "unit": "px" },
				"Tablet": { "value": 0, "unit": "px" },
				"Mobile": { "value": 0, "unit": "px" },
				"isResponsive": false
			}
		},
		"ratio": {
			"type": "object",
			"default": {
				"Desktop": { "value": ["none", null] },
				"Tablet": { "value": ["none", null] },
				"Mobile": { "value": ["none", null] },
				"isResponsive": false
			}
		},
		"rotate": {
			"type": "object",
			"default": {
				"Desktop": { "value": 0, "unit": "deg" },
				"Tablet": { "value": 0, "unit": "deg" },
				"Mobile": { "value": 0, "unit": "deg" },
				"isResponsive": false
			}
		},
		"blockName": {
			"type": "string"
		}
	},
	"textdomain": "monoblock",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
